{
  "name": "eval32",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "gulp",
    "build": "gulp build"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.14.5",
    "autoprefixer": "^10.2.5",
    "browser-sync": "^2.26.14",
    "cssnano": "^5.0.2",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-changed": "^4.0.3",
    "gulp-dart-sass": "^1.0.2",
    "gulp-file-include": "^2.3.0",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-postcss": "^9.0.0",
    "gulp-uglify": "^3.0.2",
    "vinyl-named": "^1.1.0",
    "webpack-stream": "^6.1.1"
  },
  "dependencies": {
    "bootstrap": "^5.0.1",
    "dropzone": "^5.9.2",
    "feather-icons": "^4.28.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "import/first": "off",
      "indent": [
        "error",
        2
      ],
      "no-console": "off",
      "no-unused-vars": "warn",
      "padded-blocks": "off",
      "prefer-promise-reject-errors": "off",
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    },
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
